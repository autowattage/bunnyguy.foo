---
import '../styles/threeblocksnav.css';
import type { ImageMetadata } from 'astro';
import { Image } from 'astro:assets';
const images = import.meta.glob<{ default: ImageMetadata }>('/src/assets/*.{jpeg,jpg,png,gif}');
const {img1, img2, img3, img1alt, img2alt, img3alt} = Astro.props;
---
<aside class="threeblocksnav">
  <Image src={images[img1]()} alt={img1alt} />
  <div>
    <Image src={images[img2]()} alt={img2alt} />
    <Image src={images[img3]()} alt={img3alt} />
  <div>
</aside>
